programa {
  funcao inicio() {
    //declaracao
    real valor1,valor2,valor3, media_aritmetica
    //entrada de dados
    escreva("informe valor1")
    escreva(valor1)
    leia(valor1)
    escreva("informe valor2")
    escreva(valor2)
    leia(valor2)
    escreva("informe valor3")
    escreva(valor3)
    leia(valor3)
    //processamento
    media_aritmetica= valor1+valor2+valor3/3
    //saida de dados
    escreva("media do valor gasto")
    escreva(media_aritmetica)
  }
}
