programa {
  funcao inicio() {
    //declaração de variaveis 
    real celcius,fahrenheit,temperatura

    //Entrada de dados
    escreva("Informe a temperatura em celcius")
    leia(celcius)


    //Processamento
    fahrenheit=celcius*9/5+32

    //Saida de dados
    escreva("a temperatura em fahrenheit é ", fahrenheit)

  }
}
